<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>
      <add key="provider" value="System.Data.SqlClient" />
    </appSettings>
    <connectionStrings>
      <add name="BookshelfSqlProvider" connectionString ="Data Source=(local)\SQLEXPRESS;
          Initial Catalog=Bookshelf;Integrated Security=True" />
      <add name="BookshelfOleDbProvider" connectionString ="Data Source=(local)\SQLEXPRESS;
          Initial Catalog=Bookshelf;Integrated Security=SSPI" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
    </startup>
</configuration>